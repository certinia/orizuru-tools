{
	"package": {
		"dependencies": {
			"@financialforcedev/orizuru": "^5.0.2",
			"@financialforcedev/orizuru-auth": "^3.0.3",
			"@financialforcedev/orizuru-openapi": "^2.0.1",
			"@financialforcedev/orizuru-transport-rabbitmq": "^3.0.3",
			"debug-plus": "1.2.2",
			"fs-extra": "4.0.2",
			"jsdoc": "3.5.5",
			"jsforce": "1.8.0",
			"klaw-sync": "3.0.0",
			"lodash": "4.17.4",
			"pkginfo": "0.4.1",
			"throng": "4.0.0",
			"uuid": "3.1.0"
		},
		"devDependencies": {
			"@financialforcedev/eslint-config": "^3.0.0",
			"chai": "4.1.1",
			"chai-as-promised": "7.1.1",
			"mocha": "3.5.0",
			"nyc": "11.2.1",
			"proxyquire": "1.8.0",
			"sinon": "3.2.1",
			"sinon-chai": "2.14.0"
		},
		"scripts": {
			"doc": "jsdoc -c config/jsdoc.json",
			"generate-apex-transport": "orizuru setup generate-apex-transport src/node/lib/schema src/apex/app/main/default/classes",
			"lint-fix": "eslint src/node --fix",
			"orizuru-post-init": "echo 'No post init steps'",
			"pretest": "eslint src/node",
			"single-test": "nyc --all=false mocha",
			"test": "nyc mocha --recursive src/node/spec"
		},
		"nyc": {
			"check-coverage": true,
			"per-file": true,
			"lines": 100,
			"statements": 100,
			"functions": 100,
			"branches": 100,
			"include": [
				"src/node"
			],
			"reporter": [
				"lcov",
				"html",
				"text",
				"text-summary"
			],
			"cache": true,
			"all": true
		}
	}
}
