{
  "name": "@financialforcedev/orizuru-tools",
  "version": "1.0.0",
  "description": "Development tools for the Orizuru framework.",
  "bin": {
    "orizuru": "src/lib/bin/cli.js"
  },
  "main": "lib/src/index.js",
  "scripts": {
    "pretest": "eslint src",
    "single-test": "nyc --all=false mocha",
    "test": "nyc --include src mocha --recursive src/spec",
    "posttest": "nyc --include templates/web-worker-template/res/src mocha --recursive templates/web-worker-template/res/src/node/spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/financialforcedev/orizuru-tools"
  },
  "keywords": [
    "Orizuru",
    "tools"
  ],
  "author": "FinancialForce",
  "license": "BSD-3-Clause",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "reporter": [
      "lcov",
      "html",
      "text",
      "text-summary"
    ],
    "cache": true,
    "all": true
  },
  "devDependencies": {
    "@financialforcedev/eslint-config": "^3.0.1",
    "@financialforcedev/orizuru": "^5.1.0",
    "@financialforcedev/orizuru-openapi": "^2.0.2",
    "@financialforcedev/orizuru-transport-rabbitmq": "^3.0.3",
    "app-root-path": "2.0.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "eslint": "4.9.0",
    "mocha": "4.0.1",
    "nyc": "11.2.1",
    "pkginfo": "^0.4.1",
    "proxyquire": "1.8.0",
    "sinon": "4.0.1",
    "sinon-chai": "2.14.0",
    "throng": "^4.0.0"
  },
  "dependencies": {
    "avsc": "5.0.6",
    "bluebird": "3.5.1",
    "cliui": "3.2.0",
    "debug": "^3.1.0",
    "debug-plus": "1.2.2",
    "debug-stream": "^3.0.1",
    "fs-extra": "4.0.2",
    "init-package-json": "1.10.1",
    "inquirer": "3.3.0",
    "js-yaml": "3.10.0",
    "jsforce": "1.8.0",
    "klaw-sync": "3.0.2",
    "lodash": "4.17.4",
    "yargs": "9.0.1"
  }
}
